<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WH2.Ausgabe"
             Title="Ausgabe">
    <VerticalStackLayout AnchorX="100">
        
        <Button Text="Eingabe!"
        
        BorderWidth="1"
        HorizontalOptions="Center"
        VerticalOptions="CenterAndExpand"
        Clicked="GeheZuAusgabe" AnchorX="500" />

        <Button Text="Ausgabe!"
        
        BorderWidth="1"
        HorizontalOptions="Center"
        VerticalOptions="CenterAndExpand"
        Clicked="GeheZuEingabe" AnchorX="500" />
    </VerticalStackLayout>
</ContentPage>